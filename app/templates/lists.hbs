

<div class="list-container">
    <div class="infinite-width lists">
        {{#each model as |list|}}
            <!-- AquÃ­  -->
            {{#trello-list list=list action="addItem"}} {{/trello-list}}
        {{/each}}
        <div class="new-list">
            <form {{ action "create" on="submit" }}>
